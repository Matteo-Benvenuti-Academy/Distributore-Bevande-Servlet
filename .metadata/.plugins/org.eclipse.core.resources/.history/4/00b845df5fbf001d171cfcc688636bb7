	<h1>Elenco bevande disponibili</h1>

	<%
		List<Bevanda> elenco = BevandaDao.getIstanza().findAll();
		
		String risultato = "";
		for(Studente stu: elenco){
			risultato += stu.getNominativo() + " " + stu.getMatricola() + "<br />";
		}
		
		out.write(risultato);
	%>
